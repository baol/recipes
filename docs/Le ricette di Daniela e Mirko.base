filters:
  and:
    - file.ext == "md"
formulas:
  Untitled: ""
  Picture: file.embeds[0]
views:
  - type: cards
    name: Table
    filters:
      and:
        - '!file.folder.endsWith("Templates")'
        - file.basename != "index"
        - file.basename != "README"
    order:
      - file.name
      - file.folder
      - tags
    sort:
      - property: file.folder
        direction: ASC
    columnSize:
      file.name: 419
      file.folder: 228
      file.tags: 117
    image: formula.Picture
    cardSize: 170
